<% page.posts.each(function(post){ %>
  <article class="post">
      <% if (post.title){ %>
        <% if (post.link){ %>
          <h1><a href="<%= post.link %>"><%= post.title %></a></h1>
        <% } else { %>
          <h1><a href="<%- url_for(post.path) %>"><%= post.title %></a></h1>
        <% } %>
      <% } %>
      <div class="post-content ">
        <%- partial('_partial/post/gallery', {post: post}) %>
        <%- post.content %>
      </div>
  </article>
<% }) %>
<% if (page.total > 1){ %>
  <%- partial('_partial/pagination') %>
<% } %>
